{
    "version": 2,
    "builds": [
      {
        "src": "./api/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/publicacao",
        "methods": ["GET", "POST", "PUT"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET, POST, PUT",
          "Access-Control-Allow-Headers": "Content-Type"
        },
        "dest": "./api/publicacao.ts" 
      }
      
    ]
  }
  